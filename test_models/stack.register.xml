<?xml version="1.0" encoding="UTF-8"?>
<register-automaton>
  <alphabet>
    <inputs>
      <symbol name="IPush">
        <param type="int" name="p0"/>
      </symbol>
      <symbol name="IPop">
        <param type="int" name="p0"/>
      </symbol>
      <symbol name="IDummy"/>
    </inputs>
    <outputs>
      <symbol name="OOK"/>
      <symbol name="OFinal"/>
    </outputs>
  </alphabet>
  <constants/>
  <globals>
    <variable type="int" name="r1">1</variable>
    <variable type="int" name="r2">2</variable>
  </globals>
  <locations>
    <location name="q0" initial="true" />
    <location name="q1"/>
  </locations>
  <transitions>
    <transition from="q0" to="q1" symbol="IPush" params="p">
      <assignments>
        <assign to="r2">p</assign>
      </assignments>
    </transition>
    <transition from="q1" to="q0" symbol="IPop"/>
  </transitions>
</register-automaton>