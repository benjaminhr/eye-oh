<?xml version="1.0" encoding="UTF-8" ?>
<register-automaton>
	<alphabet>
		<inputs>
			<symbol name="IKnownReceive">
				<param name="p0" type="int" />
				<param name="p1" type="int" />
			</symbol>
			<symbol name="IFreshReceive">
				<param name="p0" type="int" />
				<param name="p1" type="int" />
			</symbol>
			<symbol name="ISet">
				<param name="p0" type="int" />
			</symbol>
			<symbol name="IKnownSend">
				<param name="p0" type="int" />
				<param name="p1" type="int" />
			</symbol>
			<symbol name="ITau">
			</symbol>
			<symbol name="IFreshSend">
				<param name="p0" type="int" />
				<param name="p1" type="int" />
			</symbol>
		</inputs>
		<outputs>
		</outputs>
	</alphabet>
	<globals>
		<variable type="int" name="x0">0</variable>
		<variable type="int" name="x1">0</variable>
	</globals>
	<locations>
		<location name="s0" initial="true" />
		<location name="s3" />
		<location name="s2" />
		<location name="s1" />
		<location name="s5" />
		<location name="s4" />
	</locations>
	<transitions>
		<transition from="s0" to="s2" symbol="ISet" params="p0">
			<assignments>
				<assign to="p0">x0</assign>
			</assignments>
		</transition>
		<transition from="s2" to="s3" symbol="IFreshReceive" params="p0,p1">
			<guard>p0 == x0 &amp;&amp; p1 != x0
			</guard>
			<assignments>
				<assign to="p1">x1</assign>
			</assignments>
		</transition>
		<transition from="s2" to="s1" symbol="ITau"></transition>
		<transition from="s2" to="s4" symbol="IFreshSend" params="p0,p1">
			<guard>p0 == x0 &amp;&amp; p1 != x0
			</guard>
			<assignments>
				<assign to="p1">x1</assign>
			</assignments>
		</transition>
		<transition from="s2" to="s5" symbol="IKnownReceive" params="p0,p1">
			<guard>p0 == x0 &amp;&amp; p1 == x0
			</guard>
		</transition>
		<transition from="s3" to="s1" symbol="IFreshSend" params="p0,p1">
			<guard>p0 == x0 &amp;&amp; p1 != x0 &amp;&amp; p1 != x1
			</guard>
			<assignments>
				<assign to="-1000000">x0</assign>
				<assign to="-1000000">x1</assign>
			</assignments>
		</transition>
		<transition from="s4" to="s1" symbol="IFreshReceive" params="p0,p1">
			<guard>p0 == x0 &amp;&amp; p1 != x0 &amp;&amp; p1 != x1
			</guard>
			<assignments>
				<assign to="-1000000">x0</assign>
				<assign to="-1000000">x1</assign>
			</assignments>
		</transition>
		<transition from="s4" to="s1" symbol="IKnownReceive" params="p0,p1">
			<guard>p0 == x0 &amp;&amp; p1 == x1
			</guard>
			<assignments>
				<assign to="-1000000">x0</assign>
				<assign to="-1000000">x1</assign>
			</assignments>
		</transition>
		<transition from="s4" to="s1" symbol="IKnownReceive" params="p0,p1">
			<guard>p0 == x0 &amp;&amp; p1 == x0
			</guard>
			<assignments>
				<assign to="-1000000">x0</assign>
				<assign to="-1000000">x1</assign>
			</assignments>
		</transition>
		<transition from="s5" to="s1" symbol="IFreshSend" params="p0,p1">
			<guard>p0 == x0 &amp;&amp; p1 != x0
			</guard>
			<assignments>
				<assign to="-1000000">x0</assign>
			</assignments>
		</transition>
