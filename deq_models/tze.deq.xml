<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dra SYSTEM "dra.dtd">
<dra>
  <states>
    <state>
      <id>s0</id>
      <available-registers />
    </state>

    <state>
      <id>s1</id>
      <available-registers>
        <register>1</register>
      </available-registers>
    </state>

    <state>
      <id>s2</id>
      <available-registers>
        <register>1</register>
      </available-registers>
    </state>
  </states>

  <initial-state>s0</initial-state>

  <transitions>
    <transition>
      <from>s0</from>
      <input>input</input>
      <op>LFresh</op>
      <register>1</register>
      <to>s1</to>
    </transition>

    <transition>
      <from>s1</from>
      <input>output</input>
      <op>LFresh</op>
      <register>1</register>
      <to>s2</to>
    </transition>

  </transitions>
</dra>